    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.4.1 < 2.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 1.3.1 < 3.0.0"
    },
    {
      "name": "stahnma/epel",
